  "scripts": {
    "build": "dotenv -e .env -- drizzle-kit migrate && dotenv -e .env -- tsx src/server/db/seed.ts && dotenv -e .env -- next build",
    "check": "biome check .",
    "check:unsafe": "biome check --write --unsafe .",
    "check:write": "biome check --write .",
    "db:generate": "npm run with-env -- drizzle-kit generate",
    "db:migrate": "npm run with-env -- drizzle-kit migrate",
    "db:push": "npm run with-env -- drizzle-kit push",
    "db:seed": "npm run with-env -- tsx src/server/db/seed.ts",
    "db:studio": "npm run with-env -- drizzle-kit studio",
    "dev": "npm run with-env -- next dev --turbo",
    "preview": "next build && npm run with-env -- next start",
    "start": "npm run with-env -- next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e .env --"
  },
